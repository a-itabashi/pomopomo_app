<% if mixcloud_response.present? %>
  <% mixcloud_response.each do |list| %>
    <div>
      <%= list["name"] %>
    </div>
    <div>
      <img src="<%= list["pictures"]["large"] %>">
    </div>
    <div>
      <% videoid = "https://www.mixcloud.com/widget/iframe/?feed=#{list['url']}" %>
      <%= link_to "この曲でpomopomoスタート！", musics_play_path(title: list["name"], videoid: videoid) %>
    </div>
  <% end %>
<% end %>

<% if youtube_response.present? %>
  <% youtube_response.each do |list| %>
    <div>
      <%= list.snippet.title %>
    </div>
    <div>
      <img src="<%= list.snippet.thumbnails.medium.url %>">
    </div>
    <div>
      <% videoid = "https://www.youtube.com/embed/#{list.id.video_id}" %>
      <%= link_to "この曲でpomopomoスタート！", musics_play_path(title: list.snippet.title, videoid: videoid) %>
    </div>
  <% end %>
<% end %>